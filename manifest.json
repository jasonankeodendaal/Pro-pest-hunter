{
  "name": "Pro Pest Hunters",
  "short_name": "Pest Hunters",
  "description": "Premium local pest control services for the Lowveld area.",
  "id": "/",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "browser"],
  "background_color": "#eef2ee",
  "theme_color": "#4CAF50",
  "orientation": "portrait-primary",
  "launch_handler": {
    "client_mode": "auto"
  },
  "icons": [
    {
      "src": "https://i.ibb.co/zHBzVwRV/image.png",
      "sizes": "1024x1024",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Book Service",
      "short_name": "Book",
      "description": "Book a pest control service",
      "url": "/services",
      "icons": [{ "src": "https://i.ibb.co/zHBzVwRV/image.png", "sizes": "192x192" }]
    },
    {
      "name": "Contact Us",
      "short_name": "Contact",
      "description": "Get in touch with us",
      "url": "/contact",
      "icons": [{ "src": "https://i.ibb.co/zHBzVwRV/image.png", "sizes": "192x192" }]
    }
  ],
  "share_target": {
    "action": "/contact",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+pest",
      "url": "/?ref=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/json": [".json"]
      }
    }
  ],
  "screenshots": [
    {
      "src": "https://images.unsplash.com/photo-1560250097-0b93528c311a?auto=format&fit=crop&q=80&w=1280",
      "sizes": "1280x720",
      "type": "image/jpeg",
      "form_factor": "wide",
      "label": "Desktop Home"
    },
    {
      "src": "https://images.unsplash.com/photo-1560250097-0b93528c311a?auto=format&fit=crop&q=80&w=720",
      "sizes": "720x1280",
      "type": "image/jpeg",
      "form_factor": "narrow",
      "label": "Mobile View"
    }
  ]
}